<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.twave.myapplication.mapper.MailMapper">
    <insert id="getLock">
        INSERT INTO tb_send_lock
        VALUES (#{userName})
    </insert>
    <delete id="releaseLock">
        DELETE
        FROM tb_send_lock
        WHERE user_name = #{userName}
    </delete>
</mapper>